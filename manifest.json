 {
    "manifest_version": 2,
    "name": "Password Voyeur",
    "short_name": "Password Voyeur",
    "version": "0.0.0.1",
    "description": "Share your password.",
    "permissions": [
        "file:///*/*", 
        "http://*/*", 
        "https://*/*"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "js": [
            "js/jquery-1.11.1.min.js",
            "js/password-voyeur.js"
        ],
        "matches": ["file:///*/*", "http://*/*", "https://*/*"],
        "run_at": "document_end"
    }]
}
